{
  "name": "floraccess",
  "version": "2.0.0",
  "description": "Authentification API",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "test": "jest --runInBand",

    "db-start": "docker build -t floraccess-db-image ./Database --build-arg postgres && docker run --name floraccess-db-container -p 18100:5432 -e POSTGRES_PASSWORD=postgres -d floraccess-db-image",
    "db-stop": "docker container stop floraccess-db-container && docker container rm floraccess-db-container && docker image rm floraccess-db-image",
    "db-logs": "docker logs floraccess-db-container",
    "db-connect": "psql -U postgres -h localhost floraccess -p 18100"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FloRobart/Home_server_maison.git"
  },
  "author": "Floris Robart",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/FloRobart/Home_server_maison/issues"
  },
  "homepage": "https://github.com/FloRobart/Home_server_maison#readme",
  "devDependencies": {
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.15.23",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.2",
    "@types/cors": "^2.8.17",
    "eslint": "^9.27.0",
    "install": "^0.13.0",
    "nodemon": "^3.1.10",
    "npm": "^11.4.1",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.3"
  },
  "dependencies": {
    "argon2": "^0.44.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "cors": "^2.8.5"
  }
}
